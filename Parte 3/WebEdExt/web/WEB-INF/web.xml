<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <filter>
        <filter-name>DeviceFilter</filter-name>
        <filter-class>Filters.DeviceFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>DeviceFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Logic.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Registrarse</servlet-name>
        <servlet-class>Logic.Registrarse</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>Logic.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Curso</servlet-name>
        <servlet-class>Logic.Curso</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Instituto</servlet-name>
        <servlet-class>Logic.Instituto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Usuario</servlet-name>
        <servlet-class>Logic.Usuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Edicion</servlet-name>
        <servlet-class>Logic.Edicion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProgramaFormacion</servlet-name>
        <servlet-class>Logic.ProgramaFormacion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Comentario</servlet-name>
        <servlet-class>Logic.Comentario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Valoracion</servlet-name>
        <servlet-class>Logic.Valoracion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Calificacion</servlet-name>
        <servlet-class>Logic.Calificacion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Certificado</servlet-name>
        <servlet-class>Logic.Certificado</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Registrarse</servlet-name>
        <url-pattern>/Registrarse</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Curso</servlet-name>
        <url-pattern>/Curso</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Instituto</servlet-name>
        <url-pattern>/Instituto</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Usuario</servlet-name>
        <url-pattern>/Usuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Edicion</servlet-name>
        <url-pattern>/Edicion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProgramaFormacion</servlet-name>
        <url-pattern>/ProgramaFormacion</url-pattern>
    </servlet-mapping>
